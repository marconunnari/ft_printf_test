"%d" 42
"%d" INT_MIN
"%d" INT_MAX
"%i" INT_MAX
"%S" 'L"Heelllooo wooorld 㑄㑅㑆𐀀"'
"%S" 'L"Aሴ𐀀"'
"%Sseeeeeeee" 'L"Aሴ𐀀"'
"%c char" "'a'"
"%c char" "'z'"
"%c char" "'\\0'"
"%c char" "'\\n'"
"%%"
"%%%% %%%s" '"percent"'
"%C" "L'ሴ'"
"%C" "L'A'"
"%C" "L'𐀀'"
